{
    "name": "omegacode/t3-debugger-utility-standalone",
    "description": "A standalone port of the DebuggerUtility of the well known CMS TYPO3.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "debug",
        "var_dump",
        "typo3"
    ],
    "authors": [
        {
            "name": "Wolf Utz",
            "email": "wpu@hotmail.de"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "~0.11.8"
    },
    "autoload": {
        "psr-4": {
            "OmegaCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmegaCode\\Tests\\": "tests/"
        }
    }
}
